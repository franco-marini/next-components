{
  "name": "react-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./**/*.{ts,tsx,test.ts,test.tsx}\"",
    "format": "prettier --write './**/*.{ts,tsx,test.ts,test.tsx}' --config ./.prettierrc",
    "lint:fix": "eslint --fix \"./**/*.{ts,tsx,test.ts,test.tsx}\" && stylelint \"./src/**/*.css\" --fix && yarn format",
    "pre-commit": "yarn type-check && yarn lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.3",
    "eslint": "8.39.0",
    "eslint-config-next": "^13.4.1",
    "less": "^4.1.3",
    "next": "^13.4.1",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-aria": "^3.24.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-stately": "^3.22.0",
    "styled-components": "^5.3.10",
    "typescript": "5.0.4",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-interactions": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/addon-styling": "^1.0.6",
    "@storybook/blocks": "^7.0.8",
    "@storybook/nextjs": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-styled-components": "^2.1.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "storybook": "^7.0.8"
  },
  "resolutions": {
    "styled-components": "^5"
  }
